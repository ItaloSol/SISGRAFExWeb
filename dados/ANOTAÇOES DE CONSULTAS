/
                BUSCA PELO CODIGO DO PRODUTO EM ORÇAMENTO 
                        SELECT cod_produto 
                     tipo_produto, 
                    descricao_produto, 
                    quantidade ,
                     preco_unitario 
                     FROM tabela_produtos_orcamento
                      WHERE cod_orcamento = '6099'
/
//
/// 
////
/////
//////
////////
//////////
////////////
//////////////
                        CALCULO QUANTIDADE X PREÇO UNITARIO  TABELA PRODUTOS_ORCAMENTO
                    SELECT (quantidade * preco_unitario) AS VLR_PARC "
                    + "FROM tabela_produtos_orcamento "
                    + "WHERE cod_orcamento = ? AND cod_produto = ? AND tipo_produto = ?");          
